CREATE TABLE WAREHOUSE_INVENTORY(
    WH_ID INT,
    PRODUCT_ID INT,
    BIN VARCHAR2(255),
    ZONE VARCHAR2(255),
    UNITS INT,
    CONSTRAINT WAREHOUSE_INVENTORY_PK PRIMARY KEY(WH_ID),
    CONSTRAINT WAREHOUSE_INVENTORY_FK1 FOREIGN KEY(PRODUCT_ID) REFERENCES PRODUCT(PRODUCT_ID),
    CONSTRAINT WAREHOUSE_INVENTORY_FK2 FOREIGN KEY(BIN) REFERENCES APPROVED_BIN(BIN),
    CONSTRAINT WAREHOUSE_INVENTORY_FK3 FOREIGN KEY(ZONE) REFERENCES APPROVED_ZONE(ZONE)
);
