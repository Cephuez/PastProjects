CREATE TABLE ORDER_LIST(
    ID INT,
    ORDER_ID INT, 
    PRODUCT_ID INT,
    UNITS INT,
    BIN VARCHAR2(255),
    ZONE VARCHAR2(255),
    CONSTRAINT ORDER_LIST_PK PRIMARY KEY(ID),
    CONSTRAINT ORDER_LIST_FK1 FOREIGN KEY(ORDER_ID) REFERENCES ORDERS(ORDER_ID),
    CONSTRAINT ORDER_LIST_FK2 FOREIGN KEY(PRODUCT_ID) REFERENCES PRODUCT(PRODUCT_ID),
    CONSTRAINT ORDER_LIST_FK3 FOREIGN KEY(BIN) REFERENCES APPORVED_BIN(BIN),
    CONSTRAINT ORDER_LIST_FK4 FOREIGN KEY(ZONE) REFERENCES APPROVED_ZONE(ZONE)
)
