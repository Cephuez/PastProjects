CREATE TABLE EMPLOYEES(
    EMPLOYEE_ID INT,
    FIRST_NAME VARCHAR2(255),
    LAST_NAME VARCHAR2(255),
    DEPARTMENT_ID INT,
    HIRE_DATE DATE,
    JOB_ID INT,
    CONSTRAINT EMP_PK PRIMARY KEY(EMPLOYEE_ID),
    CONSTRAINT EMP_FK1 FOREIGN KEY(DEPARTMENT_ID) REFERENCES DEPARTMENT(DEPARTMENT_ID),
    CONSTRAINT EMP_FK2 FOREIGN KEY(JOB_ID) REFERENCES JOB_TITLE(JOB_ID)
);
