create or replace PACKAGE PACKAGE_EMPLOYEE_LOGIN
AS
    PROCEDURE CREATE_NEW_USER(EMP_ID INT, U_NAME VARCHAR2);
    FUNCTION GET_USER_ID(U_NAME VARCHAR2) RETURN NUMBER;
END PACKAGE_EMPLOYEE_LOGIN;

create or replace PACKAGE BODY PACKAGE_EMPLOYEE_LOGIN
AS
    PROCEDURE CREATE_NEW_USER(EMP_ID INT, U_NAME VARCHAR2)
    IS
    BEGIN
    INSERT INTO EMPLOYEE_LOGIN(ID, EMPLOYEE_ID, USER_NAME)
    VALUES(1, EMP_ID, U_NAME);

    for x in(SELECT TNAME FROM TAB)
    LOOP
        DBMS_OUTPUT.PUT_LINE(x.TNAME);
    END LOOP;
    END;

    FUNCTION GET_USER_ID(U_NAME VARCHAR2)
    RETURN NUMBER
    IS
    EMP_ID NUMBER;
    BEGIN
        SELECT EMPLOYEE_ID
        INTO EMP_ID
        FROM EMPLOYEE_LOGIN
        WHERE USER_NAME = U_NAME;
        RETURN EMP_ID;
    END;
END;
