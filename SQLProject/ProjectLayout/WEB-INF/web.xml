		<url-pattern>/AddIndividualServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>InsertDates</servlet-name>
		<url-pattern>/InsertDates</url-pattern>
	</servlet-mapping>
