CREATE TABLE ORDERS(
    ORDER_ID INT,
    CUSTOMER_ID INT,
    EMPLOYEE_ID INT,
    ORDER_DATE DATE,
    SHIP_DATE DATE,
    CONSTRAINT ORDERS_PK PRIMARY KEY(ORDER_ID),
    CONSTRAINT ORDERS_FK1 FOREIGN KEY(CUSTOMER_ID) REFERENCES CUSTOMERS(CUSTOMER_ID),
    CONSTRAINT ORDERS_FK2 FOREIGN KEY(EMPLOYEE_ID) REFERENCES EMPLOYEES(EMPLOYEE_ID)
);
