CREATE TABLE PICKS(
    ID INT,
    ORDER_ID INT,
    PRODUCT_ID INT,
    QUANTITY INT,
    EMPLOYEE_ID INT,
    PICK_STATUS VARCHAR2(1) DEFAULT 'N' CHECK(PICK_STATUS IN('Y','N')),
    TIME_PICKED DATE,
    BIN VARCHAR2(255),
    ZONE VARCHAR2(255),
    CONSTRAINT PICKS_PK PRIMARY KEY(ID),
    CONSTRAINT PICKS_FK1 FOREIGN KEY(ORDER_ID) REFERENCES ORDERS(ORDER_ID),
    CONSTRAINT PICKS_FK2 FOREIGN KEY(PRODUCT_ID) REFERENCES PRODUCTS(PRODUCT_ID),
    CONSTRAINT PICKS_FK3 FOREIGN KEY(EMPLOYEE_ID) REFERENCES EMPLOYEES(EMPLOYEE_ID),
    CONSTRAINT PICKS_FK4 FOREIGN KEY(BIN) REFERENCES APPROVED_BIN(BIN),
    CONSTRAINT PICKS_FK5 FOREIGN KEY(ZONE) REFERENCES APPROVED_ZONE(ZONE)
)
