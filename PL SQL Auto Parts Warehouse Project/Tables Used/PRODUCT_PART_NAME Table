CREATE TABLE PRODUCT_PART_NAME(
    P_ID INT,
    SUPPLIER_ID INT,
    PART_NUMBER INT,
    PRODUCT_NAME VARCHAR2(255),
    UNIT_PRICE NUMBER(10,2),
    CONSTRAINT PRODUCT_PART_NAME_PK1 PRIMARY KEY(P_ID),
    CONSTRAINT PRODUCT_PART_NAME_FK1 FOREIGN KEY(SUPPLIER_ID) REFERENCES SUPPLIERS(SUPPLIER_ID)
);
