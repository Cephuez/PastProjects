CREATE TABLE PRODUCT(
    PRODUCT_ID INT,
    PRODUCT_NAME VARCHAR2(255),
    UNIT_PRICE NUMBER(10,2),
    TOTAL_UNITS INT,
    CATEGORY_ID INT,
    SUPPLIER_ID INT,
    CONSTRAINT PRODUCTS_PK PRIMARY KEY(PRODUCT_ID),
    CONSTRAINT PRODUCTS_FK1 FOREIGN KEY(CATEGORY_ID) REFERENCES CATEGORIES(CATEGORY_ID),
    CONSTRAINT PRODUCTS_FK2 FOREIGN KEY(SUPPLIER_ID) REFERENCES SUPPLIERS(SUPPLIER_ID)
);
