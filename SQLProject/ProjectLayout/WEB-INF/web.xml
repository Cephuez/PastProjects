		<servlet-name>MemberProfileServlet</servlet-name>
		<url-pattern>/MemberProfileServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddFavoriteServlet</servlet-name>
		<url-pattern>/AddFavoriteServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ShowAdopterServlet</servlet-name>
		<url-pattern>/ShowAdopterServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ShowPetsServlet</servlet-name>
		<url-pattern>/ShowPetsServlet</url-pattern>
	</servlet-mapping>
</web-app>
